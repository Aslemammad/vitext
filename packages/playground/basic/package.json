{
  "name": "basic",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite serve",
    "debug": "node --inspect ./node_modules/vite/bin/vite serve",
    "build": "rm -rf dist && vite build --outDir dist && serve -s dist",
    "debug:build": "node --inspect ./node_modules/vite/bin/vite build --outDir dist",
    "ssr": "rm -rf dist && vitex ssr",
    "debug:ssr": "rm -rf dist && node --inspect-brk ./node_modules/vitext/bin/vitext.js ssr"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "peerDependencies": {
    "vitext": "latest"
  },
  "devDependencies": {
    "@babel/helper-compilation-targets": "^7.13.16",
    "@mdx-js/mdx": "^1.6.22",
    "@types/react": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "serve": "^11.3.2",
    "vite": "^2.2.3",
    "vite-plugin-mdx": "^3.3.1"
  }
}
