    "@rollup/plugin-typescript": "^8.2.0",
    "@types/mdx-js__react": "^1.5.3",
    "@types/react": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "babel-plugin-import": "^1.13.3",
    "babel-runtime": "^6.26.0",
    "chokidar": "^3.5.1",
