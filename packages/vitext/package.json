{
  "name": "vitext",
  "version": "0.0.2",
  "bin": {
    "vitext": "./bin/vitext.js"
  },
  "files": [
    "**"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "keywords": [
    "vitext",
    "vite",
    "ssg",
    "ssr",
    "react"
  ],
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aslemammad/vitext.git"
  },
  "bugs": {
    "url": "https://github.com/aslemammad/vitext/issues"
  },
  "homepage": "https://github.com/aslemammad/vitext",
  "scripts": {
    "build-node": "NODE_ENV=production rollup -c rollup.config.vite.js",
    "config:internal": "rollup -c --config-internal",
    "config:components": "rollup -c --config-components",
    "config:react": "rollup -c --config-react",
    "config:dynamic": "rollup -c --config-dynamic",
    "delete": "rollup -c --config-delete",
    "concurrent-build": "concurrently 'yarn:config:*' 'yarn:config:* --config-declaration'",
    "concurrent-dev": "concurrently 'yarn:config:*' 'yarn:config:* --config-declaration' 'NODE_ENV=development rollup -c rollup.config.vite.js' -w",
    "build": "cross-env-shell NODE_ENV=production \"yarn run build-node && yarn run concurrent-build\"",
    "watch": "chokidar \"src/**/*.ts\" \"src/**/*.tsx\" -c \"yarn concurrent-build\"",
    "watch-all": "chokidar \"src/**/*.ts\" \"src/**/*.tsx\" -c \"yarn concurrent-dev\"",
    "dev": "cross-env-shell NODE_ENV=development \"yarn watch-all\"",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/compression": "^1.7.1",
    "@types/cors": "^2.8.12",
    "@types/deep-equal": "^1.0.1",
    "@types/enhanced-resolve": "^3.0.6",
    "@types/fs-extra": "^9.0.8",
    "@types/http-proxy": "^1.17.7",
    "@types/minimist": "^1.2.0",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.1",
    "@types/react-loadable": "^5.5.6",
    "@types/react-router-dom": "^5.1.7",
    "@types/use-subscription": "^1.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.5",
    "async-mutex": "^0.3.1",
    "cac": "^6.7.3",
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "chokidar-cli": "^2.1.0",
    "comlink": "^4.3.1",
    "compression": "^1.7.4",
    "concurrently": "^6.0.0",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "deep-equal": "^2.0.5",
    "dequal": "^2.0.2",
    "enhanced-resolve": "^5.7.0",
    "es-module-lexer": "^0.7.1",
    "estree-walker": "^3.0.0",
    "fast-glob": "^3.2.5",
    "fs-extra": "^9.1.0",
    "global": "^4.4.0",
    "http-proxy": "^1.18.1",
    "jest-docblock": "^26.0.0",
    "jest-worker": "^27.0.6",
    "jsonfile": "^6.1.0",
    "magic-string": "^0.25.7",
    "mini-debounce": "^1.0.8",
    "minimist": "^1.2.5",
    "mitt": "^2.1.0",
    "prop-types": "^15.7.2",
    "querystring": "^0.2.1",
    "react-ssr-prepass": "^1.4.0",
    "read-pkg-up": "^7.0.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-esbuild": "0.0.0-semantic-release",
    "rollup-plugin-postcss": "^4.0.0",
    "selfsigned": "^1.10.11",
    "terser": "^5.7.1",
    "tiny-invariant": "^1.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.3",
    "use-subscription": "^1.5.1",
    "react-helmet-async": "^1.0.9",
    "esbuild": "^0.12.9",
    "postcss": "^8.3.6",
    "resolve": "^1.20.0",
    "rollup": "^2.38.5",
    "vite": "^2.4.2",
    "semantic-release": "^17.4.4"
  },
  "dependencies": {
    "use-subscription": "^1.5.1",
    "react-helmet-async": "^1.0.9",
    "esbuild": "^0.12.9",
    "postcss": "^8.3.6",
    "resolve": "^1.20.0",
    "rollup": "^2.38.5",
    "vite": "^2.4.2"
  }
}
