{
  "name": "vitext",
  "version": "0.0.1",
  "bin": {
    "vitext": "./bin/vitext.js"
  },
  "keywords": [
    "vite",
    "react"
  ],
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aslemammad/vitext.git"
  },
  "bugs": {
    "url": "https://github.com/aslemammad/vitext/issues"
  },
  "homepage": "https://github.com/csr632/vitext",
  "scripts": {
    "config:node": "rollup -c --config-node",
    "config:client": "rollup -c --config-client",
    "config:components": "rollup -c --config-components",
    "concurrent-build": "concurrently 'yarn:config:*' 'yarn:config:* --config-declaration'",
    "build": "cross-env-shell NODE_ENV=production \"yarn concurrent-build\"",
    "watch": "chokidar \"src/**/*.ts\" \"src/**/*.tsx\" -c \"yarn concurrent-build\"",
    "dev": "cross-env-shell NODE_ENV=development \"yarn watch\""
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/deep-equal": "^1.0.1",
    "@types/enhanced-resolve": "^3.0.6",
    "@types/fs-extra": "^9.0.8",
    "@types/minimist": "^1.2.0",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^6.0.0",
    "deep-equal": "^2.0.5",
    "esbuild": "^0.12.9",
    "estree-walker": "^3.0.0",
    "jsonfile": "^6.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.52.1",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-esbuild": "0.0.0-semantic-release",
    "tiny-invariant": "^1.1.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.3",
    "vite": "^2.3.6"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "chokidar": "^3.5.1",
    "dequal": "^2.0.2",
    "enhanced-resolve": "^5.7.0",
    "fast-glob": "^3.2.5",
    "fs-extra": "^9.1.0",
    "global": "^4.4.0",
    "jest-docblock": "^26.0.0",
    "mini-debounce": "^1.0.8",
    "minimist": "^1.2.5",
    "mitt": "^2.1.0",
    "querystring": "^0.2.1",
    "react-helmet-async": "^1.0.9",
    "react-ssr-prepass": "^1.4.0",
    "read-pkg-up": "^7.0.1",
    "rollup-plugin-postcss": "^4.0.0"
  }
}
